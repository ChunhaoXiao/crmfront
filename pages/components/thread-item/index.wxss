.main {
  margin-top:20rpx;
}
.header {
  display: flex;
  justify-content: space-between;
  padding: 10rpx;
}

.body {
  display: flex;
  justify-content: space-between;
  padding: 10rpx;
}

.body-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.customer-name {
  font-weight: 600;
  color: #333333;
}
.created-time {
  color: #999999;
  font-size:28rpx;
}

.title {
  color: #4E7FF5;
  font-weight: 600;
}

.button {
  width: 150rpx;
  height: 56rpx;
  border-radius: 40rpx;
  background-color: #20B759;
  color: white;
  line-height: 56rpx;
  text-align: center;
  padding: 5rpx 20rpx;
}