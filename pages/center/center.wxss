page {
  height: 100%;
}
.container {
  height: 100%;
  border: 1rpx solid yellow;
  box-sizing: border-box;
  display: flex;
  align-items: flex-end;
}

.menu {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  align-items: flex-end;
  box-sizing: border-box;
  margin-top:auto;
 
}

.menu-item {
  display: flex;
  justify-content: center;
  flex-direction: column;
  width: 25%;
  text-align: center;
}